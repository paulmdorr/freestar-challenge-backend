{
  "root": true,
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-console": "warn",
    "prettier/prettier": "warn"
  }
}
